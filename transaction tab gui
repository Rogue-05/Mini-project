from tkinter import *
import customtkinter as ct

ct.set_appearance_mode('dark')
ct.set_default_color_theme('dark-blue')

root=ct.CTk()
root.geometry("1000x1000")
root.title("geometry")

trans_screen=ct.CTkToplevel(root)
trans_screen.geometry("300x500")
trans_screen.title("transactions")

ct.CTkLabel(trans_screen,text="transactions").grid(row=1)

ct.CTkButton(trans_screen,text="Withdraw").grid(row=2)
ct.CTkButton(trans_screen,text="Deposit").grid(row=3)
ct.CTkButton(trans_screen,text="transfer").grid(row=4)


root.mainloop()
